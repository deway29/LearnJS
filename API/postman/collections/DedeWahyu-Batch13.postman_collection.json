    {
      "name": "Login",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "78717adc-148b-4e18-b244-ec269489370b",
            "exec": [
              "pm.test(\"Login successful\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response contains status and token\", function () {\r",
              "    let json = pm.response.json();\r",
              "    pm.expect(json).to.have.property(\"status\");\r",
              "    pm.expect(json).to.have.property(\"token\");\r",
              "});\r",
              "\r",
              "pm.test(\"Token is not empty\", function () {\r",
              "    let json = pm.response.json();\r",
              "    pm.expect(json.token).to.not.be.empty;\r",
              "});\r",
              "\r",
              "pm.test(\"Token is valid JWT format\", function () {\r",
              "    let token = pm.response.json().token;\r",
              "    pm.expect(token.split('.').length).to.eql(3);\r",
              "});\r",
              "\r",
              "pm.test(\"Response time is below 1000ms\", function () {\r",
              "    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
              "});\r",
              "\r",
              "let json = pm.response.json();\r",
              "pm.environment.set(\"token\", json.token);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
