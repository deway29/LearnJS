    {
      "name": "Add User",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "6e1fe5d1-2e07-4af4-84d1-94b511a05997",
            "exec": [
              "let json = pm.response.json();\r",
              "let username = JSON.parse(pm.request.body.raw).username;\r",
              "\r",
              "if (pm.response.code === 200) {\r",
              "    pm.test(\"Successful add\", () => {\r",
              "        pm.expect(json.message).to.include(username);\r",
              "    });\r",
              "}\r",
              "\r",
              "if (pm.response.code === 400) {\r",
              "    pm.test(\"Duplicate username\", () => {\r",
              "        pm.expect(json.message).to.include(username);\r",
              "        pm.expect(json.message).to.eql(`Username \"${username}\" already exists!`);\r",
              "    });\r",
              "}\r",
