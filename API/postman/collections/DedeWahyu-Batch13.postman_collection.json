    {
      "name": "List Users",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "54400246-1ed3-4960-bc2b-b43e6dee8468",
            "exec": [
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response time is less than 1 second\", function () {\r",
              "    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
              "});\r",
              "\r",
              "pm.test(\"Response body is an array\", function () {\r",
              "    let jsonData = pm.response.json();\r",
              "    pm.expect(jsonData).to.be.an(\"array\");\r",
              "});\r",
              "\r",
              "pm.test(\"Each user has _id, username, and age\", function () {\r",
              "    let jsonData = pm.response.json();\r",
              "    jsonData.forEach(user => {\r",
              "        pm.expect(user).to.have.property(\"_id\");\r",
              "        pm.expect(user).to.have.property(\"username\");\r",
              "        pm.expect(user).to.have.property(\"age\");\r",
              "    });\r",
              "});\r",
              "\r",
              "pm.test(\"Age must be greater than 0\", function () {\r",
              "    let jsonData = pm.response.json();\r",
              "    jsonData.forEach(user => {\r",
              "        pm.expect(user.age).to.be.above(0);\r",
              "    });\r",
              "});\r",
              "\r",
              "pm.test(\"Username cannot be empty\", function () {\r",
              "    let jsonData = pm.response.json();\r",
              "    jsonData.forEach(user => {\r",
              "        pm.expect(user.username).to.not.be.empty;\r",
              "    });\r",
              "});\r",
              "\r",
              "pm.test(\"Username must be unique\", function () {\r",
              "    let jsonData = pm.response.json();\r",
              "    let usernames = jsonData.map(u => u.username);\r",
              "    let unique = new Set(usernames);\r",
              "\r",
              "    pm.expect(unique.size).to.eql(usernames.length);\r",
              "});\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
